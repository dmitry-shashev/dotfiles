{
  "Console": {
    "prefix": "cc",
    "body": [
      "// @#$",
      "// eslint-disable-next-line no-console",
      "console.log($1)"
    ],
    "description": "Console Log"
  },
  "Equal": {
    "prefix": "=",
    "body": [" === "],
    "description": "Equal"
  },
  "If": {
    "prefix": "if",
    "body": ["if ($1) {", "}"],
    "description": "If"
  },
  "Const": {
    "prefix": "c",
    "body": ["const $1"],
    "description": "const"
  },
  "CallbackArray": {
    "prefix": "ca",
    "body": ["($1) => [", "]"],
    "description": "callback array"
  },
  "Callback": {
    "prefix": "cb",
    "body": ["($1) => {", "}"],
    "description": "callback"
  },
  "Callback Simplie": {
    "prefix": "cn",
    "body": ["($1) => $2"],
    "description": "callback simple"
  },
  "Callback value": {
    "prefix": "cv",
    "body": ["($1) => ({", "})"],
    "description": "callback value"
  },
  "Export default": {
    "prefix": "ed",
    "body": ["export default $1"],
    "description": "export default"
  },
  "Export": {
    "prefix": "e",
    "body": ["export $1"],
    "description": "export"
  },
  "Default": {
    "prefix": "d",
    "body": ["default $1"],
    "description": "default"
  },
  "Describe": {
    "prefix": "de",
    "body": ["describe('$1', () => {", "})"],
    "description": "default"
  },
  "It": {
    "prefix": "it",
    "body": ["it('$1', () => {", "})"],
    "description": "it"
  },
  "Function": {
    "prefix": "f",
    "body": ["function $1($2) {", "}"],
    "description": "function"
  },
  "Return": {
    "prefix": "r",
    "body": ["return $1"],
    "description": "return"
  },
  "SetInterval": {
    "prefix": "si",
    "body": ["setInterval(() => {", "$2", "}, $1)"],
    "description": "set interval"
  },
  "SetTimeout": {
    "prefix": "st",
    "body": ["setTimeout(() => {", "$2", "}, $1)"],
    "description": "set timeout"
  },
  "useState": {
    "prefix": "us",
    "body": [
      "const [${1}, set${1/(.*)/${1:/capitalize}/}] = useState($2);",
      "$0"
    ],
    "description": "use state"
  },
  "useEffect": {
    "prefix": "ue",
    "body": ["useEffect(() => {", "$1", "}, [])"],
    "description": "use effect"
  },
  "useCallback": {
    "prefix": "uc",
    "body": ["useCallback(() => {", "$1", "}, [])"],
    "description": "use callback"
  },
  "useMemo": {
    "prefix": "um",
    "body": ["useMemo(() => {", "$1", "}, [])"],
    "description": "use memo"
  }
}
